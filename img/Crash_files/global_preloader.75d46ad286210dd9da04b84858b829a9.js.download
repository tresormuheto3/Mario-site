var disableAutoHideGlobalPreloader=false;function showGlobalPreloader(){window.global_preloader_hided=false;document.querySelector(".main-preloader").style.display="flex"}function hideGlobalPreloader(){if(window.global_preloader_hided){return}document.querySelector(".main-preloader").style.display="none";window.global_preloader_hided=true;var a;if(typeof Event==="function"){a=new Event("hide_global_preloader")}else{a=document.createEvent("Event");a.initEvent("hide_global_preloader",true,true)}window.dispatchEvent(a)}window.addEventListener("load",function(){if(!disableAutoHideGlobalPreloader){hideGlobalPreloader()}});